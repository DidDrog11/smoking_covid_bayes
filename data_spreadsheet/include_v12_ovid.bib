Publisher: NA},
	keywords = {Humans, Male, Female, Adult, Middle Aged, Aged, Retrospective Studies, Tomography, X-Ray Computed, China/ep [Epidemiology], Severity of Illness Index, Hospitalization, Treatment Outcome, Travel, SARS-CoV-2, Oxygen Inhalation Therapy/mt [Methods], *COVID-19/ep [Epidemiology], COVID-19/di [Diagnosis], *COVID-19/th [Therapy], Cough/ep [Epidemiology], Cough/vi [Virology], Fever/ep [Epidemiology], Fever/vi [Virology], Respiration, Artificial/mt [Methods]},

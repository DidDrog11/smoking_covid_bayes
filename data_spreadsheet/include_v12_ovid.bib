	author = {Dobano, Carlota and Ramirez-Morros, Anna and Alonso, Selena and Vidal-Alaball, Josep and Ruiz-Olalla, Gemma and Vidal, Marta and Rubio, Rocio and Cascant, Emma and Parras, Daniel and Rodrigo Melero, Natalia and Serra, Pau and Carolis, Carlo and Santamaria, Pere and Forcada, Anna and Mendioroz, Jacobo and Aguilar, Ruth and Moncunill, Gemma and Ruiz-Comellas, Anna},

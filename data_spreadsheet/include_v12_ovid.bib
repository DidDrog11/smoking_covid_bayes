@article{nassar_outcomes_2021,
	title = {Outcomes and risk factors for death in patients with coronavirus disease-2019 ({COVID}-19) pneumonia admitted to the intensive care units of an {Egyptian} {University} {Hospital}. {A} retrospective cohort study},

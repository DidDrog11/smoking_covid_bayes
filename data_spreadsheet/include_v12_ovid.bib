	journal = {J Infect Public Health},
	author = {Nassar, Yasser and Mokhtar, Ahmed and Elhadidy, Amr and Elsayed, Marwa and Mostafa, Farouk and Rady, Ashraf and Eladawy, Akram and Elshazly, Mostafa and Saeed, Mohamed and Mokhtar, Sherif and Buschbeck, Samuel and Sakr, Yasser},

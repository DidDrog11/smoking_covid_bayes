Publisher: NA},
	keywords = {Humans, Male, Female, Adult, Middle Aged, Risk Factors, Aspartate Aminotransferases/bl [Blood], Smoking/ae [Adverse Effects], Retrospective Studies, Severity of Illness Index, Hospitalization, SARS-CoV-2, Pneumonia, Viral/mo [Mortality], Pneumonia, Viral/vi [Virology], COVID-19/mo [Mortality], *Pneumonia, Viral/th [Therapy], *COVID-19/th [Therapy], *Oxygen Inhalation Therapy, Blood Urea Nitrogen, Republic of Korea/ep [Epidemiology]},

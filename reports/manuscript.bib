	note = {Publisher: Cold Spring Harbor Laboratory Press},
	pages = {2020.06.10.20127514},
	file = {Snapshot:/Users/david/Zotero/storage/D4RVNJ7X/2020.06.10.html:text/html;Full Text PDF:/Users/david/Zotero/storage/RUDS3Z86/Miyara et al. - 2020 - Low rate of daily smokers in patients with symptom.pdf:application/pdf},
}


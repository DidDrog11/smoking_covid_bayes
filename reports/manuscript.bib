	urldate = {2021-07-23},
	journal = {Journal of Primary Care \& Community Health},
	author = {Nanda, Sanjeev and Chacin Suarez, Audry S. and Toussaint, Loren and Vincent, Ann and Fischer, Karen M. and Hurt, Ryan and Schroeder, Darrell R. and Medina Inojosa, Jose R. and O’Horo, John C. and DeJesus, Ramona S. and Abu Lebdeh, Haitham S. and Mundi, Manpreet S. and Iftikhar, Salma and Croghan, Ivana T.},

	author = {Lamure, Sylvain and Duléry, Rémy and Blasi, Roberta Di and Chauchet, Adrien and Laureana, Cécile and Deau-Fischer, Bénédicte and Drenou, Bernard and Soussain, Carole and Rossi, Cédric and Noël, Nicolas and Choquet, Sylvain and Bologna, Serge and Joly, Bertrand and Kohn, Milena and Malak, Sandra and Fouquet, Guillemette and Daguindau, Etienne and Bernard, Sophie and Thiéblemont, Catherine and Cartron, Guillaume and Lacombe, Karine and Besson, Caroline},
	month = oct,

	urldate = {2021-07-26},
	institution = {Epidemiology},
	author = {Röthlin Eriksen, Alexandra R and Fogh, Kamille and Hasselbalch, Rasmus B and Bundgaard, Henning and Nielsen, Susanne D and Jørgensen, Charlotte S and Scharff, Bibi FSS and Erikstrup, Christian and Sækmose, Susanne G. and Holm, Dorte K and Aagaard, Bitten and Kristensen, Jonas H and Bødker, Cecilie A and Norsk, Jakob and Nielsen, Pernille Brok and Østergaard, Lars and Ellermann-Eriksen, Svend and Andersen, Berit and Nielsen, Henrik and Johansen, Isik S. and Wiese, Lothar and Simonsen, Lone and K.Fischer, Thea and Folke, Fredrik and Lippert, Freddy and Ostrowski, Sisse R and Ethelberg, Steen and Koch, Anders and Vangsted, Anne-Marie and Krause, Tyra and Fomsgaard, Anders and Nielsen, Claus and Ullum, Henrik and Skov, Robert and Iversen, Kasper},

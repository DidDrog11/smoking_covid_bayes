	urldate = {2020-07-21},
	journal = {medRxiv},
	author = {Al-Hindawi, Ahmed and Sokhi, Jagdish and Cuddihy, Joshua and Lockie, Christopher and Christie, Linsey and Davies, Roger and Singh, Suveer and Vizcaychipi, Marcela and Hayes, Michelle and Sisson, Alice and Keays, Richard},

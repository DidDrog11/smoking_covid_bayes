	urldate = {2021-07-26},
	institution = {Public and Global Health},
	author = {Genecand, Camille and Mongin, Denis and Koegler, Flora and Lebowitz, Dan and Regard, Simon and Nehme, Mayssam and Braillard, Olivia and Grira, Marwène and Joubert, Dominique and Chopard, Pierre and Delaporte, Elisabeth and Stirnemann, Jerome and Guessous, Idris and Tardin, Aglaé and Courvoisier, Delphine S.},
	month = may,
	year = {2021},
	doi = {10.1101/2021.05.24.21256813},
	file = {Submitted Version:/Users/david/Zotero/storage/QWVZ5IPU/Genecand et al. - 2021 - Cohort profile Actionable Register of Geneva Out-.pdf:application/pdf},

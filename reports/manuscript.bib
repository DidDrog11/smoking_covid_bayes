	month = jun,
	year = {2020},
	note = {Publisher: Cold Spring Harbor Laboratory Press},
	pages = {2020.06.09.20126516},
	file = {Snapshot:/Users/david/Zotero/storage/GGIRSDTV/2020.06.09.html:text/html;Full Text PDF:/Users/david/Zotero/storage/GLJ8APK4/Hultcrantz et al. - 2020 - COVID-19 infections and outcomes in patients with .pdf:application/pdf},
}


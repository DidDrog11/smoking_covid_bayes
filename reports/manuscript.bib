	author = {Printza, Athanasia and Katotomichelakis, Mihalis and Valsamidis, Konstantinos and Metallidis, Symeon and Panagopoulos, Periklis and Panopoulou, Maria and Petrakis, Vasilis and Constantinidis, Jannis},
	month = mar,

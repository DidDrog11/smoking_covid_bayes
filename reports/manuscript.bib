	note = {Publisher: Cold Spring Harbor Laboratory Press},
	pages = {2020.06.08.20125823},
	file = {Snapshot:/Users/david/Zotero/storage/837P5HVY/2020.06.08.html:text/html;Full Text PDF:/Users/david/Zotero/storage/9S43KUQX/Veras et al. - 2020 - SARS-CoV-2 triggered neutrophil extracellular trap.pdf:application/pdf},

	keywords = {Humans, Male, Female, Adult, Middle Aged, Prevalence, Risk Factors, Aged, Age Factors, Retrospective Studies, China/ep [Epidemiology], *Anxiety Disorders, *Disabled Persons, *Survivors, Activities of Daily Living, Anxiety Disorders/ep [Epidemiology], Anxiety Disorders/px [Psychology]},

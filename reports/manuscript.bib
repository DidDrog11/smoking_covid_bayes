	author = {Zuo, Yu and Estes, Shanea K. and Gandhi, Alex A. and Yalavarthi, Srilakshmi and Ali, Ramadan A. and Shi, Hui and Sule, Gautam and Gockman, Kelsey and Madison, Jacqueline A. and Zuo, Melanie and Woodard, Wrenn and Lezak, Sean P. and Lugogo, Njira L. and Kanthi, Yogendra and Knight, Jason S.},
	month = jun,
	year = {2020},
	note = {Publisher: Cold Spring Harbor Laboratory Press},

	urldate = {2020-07-21},
	journal = {medRxiv},
	author = {Gil-Agudo, Angel and Rodriguez-Cola, Miguel and Jimenez-Velasco, Irena and Gutierrez-Henares, Francisco and Lopez-Dolado, Elisa and Gambarrutta-Malfatti, Claudia and Vargas-Baquero, Eduardo},

	year = {2021},
	doi = {10.1101/2021.02.24.21252368},
	file = {Submitted Version:/Users/david/Zotero/storage/U6IAHBV4/Shields et al. - 2021 - Longitudinal protection following natural SARS-CoV.pdf:application/pdf},
}


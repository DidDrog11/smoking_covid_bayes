@article{zhou_prolonged_2020,
	title = {Prolonged {SARS}-{CoV}-2 {Viral} {Shedding} in {Patients} with {COVID}-19 was {Associated} with {Delayed} {Initiation} of {Arbidol} {Treatment}: a retrospective cohort study},

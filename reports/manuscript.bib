	urldate = {2021-07-26},
	institution = {Infectious Diseases (except HIV/AIDS)},
	author = {Gornyk, Daniela and Harries, Manuela and Glöckner, Stephan and Strengert, Monika and Kerrinnes, Tobias and Bojara, Gerhard and Castell, Stefanie and Frank, Kerstin and Gubbe, Knut and Heise, Jana-Kristin and Hernandez, Pilar and Kappert, Oliver and Kern, Winfried and Illig, Thomas and Klopp, Norman and Maaß, Henrike and Ortmann, Julia and Kessel, Barbora and Roller, Gottfried and Schlüter, Monike and Tonn, Torsten and Ziemons, Michael and Kemmling, Yvonne and Lange, Berit and Krause, Gérard},
